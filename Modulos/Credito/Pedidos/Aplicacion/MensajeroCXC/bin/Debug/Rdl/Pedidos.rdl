  <Code>Function EliminaRetornos(ByVal cadena As String) As String 
	Dim CadenaResultado as String
	If(cadena &lt;&gt; "") Then
	CadenaResultado = cadena.Replace(chr(10) ," ").Replace(chr(13)," ")
	Else
	CadenaResultado =""
	End if
	return CadenaResultado
End Function</Code>
