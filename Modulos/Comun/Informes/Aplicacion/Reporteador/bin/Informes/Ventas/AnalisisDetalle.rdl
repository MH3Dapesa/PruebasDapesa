	  Function DivisionMeta(ByVal dividendo As Decimal, ByVal divisor As Decimal) As Decimal

	  If divisor=0 Then
	  Return divisor
	  End If

	  Return 1-(dividendo/divisor)
	  End Function
  
 Function IndicadorPintado(ByVal anio_evaluar As String, ByVal anio_3 As Decimal, ByVal anio_2 As Decimal, ByVal anio_1 As Decimal,  Byval anio As Decimal) As Decimal
		
		Dim resultado As Decimal = 0
		select case anio_evaluar
			Case "anio_3"
				if(anio_3 &gt; anio_2 AND anio_3 &gt; anio_1 AND anio_3 &gt; anio) Then
					resultado = 1
				Else if(anio_3 = anio_2 OR anio_3 = anio_1 OR anio_3 = anio OR anio_3 &lt; 0) then
					resultado = 0
				End if
			Case "anio_2"
				if(anio_2 &gt; anio_3 AND anio_2 &gt; anio_1 AND anio_2 &gt; anio) Then
					resultado = 1
					else if(anio_2=anio_1) then	
					resultado = 0
				else if(anio_2=anio) then	
					resultado = 0
				else if(anio_2=anio_3) and (anio_2&gt;=anio_1 AND anio_2 &gt;= anio) AND (anio_2 &gt;= 0) then
					resultado = 1			
				else
					resultado = 0
				End if
			Case "anio_1"
				if(anio_1 &gt; anio_3 AND anio_1 &gt; anio_2 AND anio_1 &gt; anio) then
					resultado = 1
				else if(anio_1=anio AND anio_1 &gt;= 0) then	
					resultado = 0
				else if(anio_1=anio_3) and (anio_1&gt;=anio_2 AND anio_1 &gt;= anio) AND (anio_1 &gt;= 0) then
					resultado = 1
				else if(anio_1=anio_2) and (anio_1&gt;=anio_3 AND anio_1 &gt;= anio) AND (anio_1 &gt;= 0) then	
					resultado = 1			
				else
					resultado = 0
				End if
			Case "anio"
				if(anio &gt; anio_3 AND anio &gt; anio_2 AND anio &gt; anio_1) then
					resultado = 1
				else if(anio=anio_3) and (anio&gt;=anio_2 AND anio &gt;= anio_1) AND (anio &gt;= 0) then
					resultado = 1
				else if(anio=anio_2) and (anio&gt;=anio_3 AND anio &gt;= anio_1) AND (anio &gt;= 0) then	
					resultado = 1
				else if(anio=anio_1) and (anio&gt;=anio_3 AND anio &gt;= anio_2) AND (anio &gt;= 0) then	
					resultado = 1
				else
					resultado = 0
				End if
		End Select
		return resultado
	End function
  
 
	Function FondoColor(ByVal anio_evaluar As String, ByVal anio_3 As Decimal, ByVal anio_2 As Decimal, ByVal anio_1 As Decimal,  Byval anio As Decimal) As String
		
		Dim resultado As String = "White"
		select case anio_evaluar
			Case "anio_3"
				if(anio_3 &gt; anio_2 AND anio_3 &gt; anio_1 AND anio_3 &gt; anio) Then
					resultado = "Yellow"
				Else if(anio_3 = anio_2 OR anio_3 = anio_1 OR anio_3 = anio OR anio_3 &lt; 0) then
					resultado = "White"
				End if
			Case "anio_2"
				if(anio_2 &gt; anio_3 AND anio_2 &gt; anio_1 AND anio_2 &gt; anio) Then
					resultado = "Yellow"
					else if(anio_2=anio_1) then	
					resultado = "White"
				else if(anio_2=anio) then	
					resultado = "White"
				else if(anio_2=anio_3) and (anio_2&gt;=anio_1 AND anio_2 &gt;= anio) AND (anio_2 &gt;= 0) then
					resultado = "Yellow"
				else
					resultado = "White"
				End if
			Case "anio_1"
				if(anio_1 &gt; anio_3 AND anio_1 &gt; anio_2 AND anio_1 &gt; anio) then
					resultado = "Yellow"
				else if(anio_1=anio AND anio_1 &gt;= 0) then	
					resultado = "White"
				else if(anio_1=anio_3) and (anio_1&gt;=anio_2 AND anio_1 &gt;= anio) AND (anio_1 &gt;= 0) then
					resultado = "Yellow"
				else if(anio_1=anio_2) and (anio_1&gt;=anio_3 AND anio_1 &gt;= anio) AND (anio_1 &gt;= 0) then	
					resultado = "Yellow"			
				else
					resultado = "White"
				End if
			Case "anio"
				if(anio &gt; anio_3 AND anio &gt; anio_2 AND anio &gt; anio_1) then
					resultado = "Yellow"
				else if(anio=anio_3) and (anio&gt;=anio_2 AND anio &gt;= anio_1) AND (anio &gt;= 0) then
					resultado = "Yellow"
				else if(anio=anio_2) and (anio&gt;=anio_3 AND anio &gt;= anio_1) AND (anio &gt;= 0) then	
					resultado = "Yellow"
				else if(anio=anio_1) and (anio&gt;=anio_3 AND anio &gt;= anio_2) AND (anio &gt;= 0) then	
					resultado = "Yellow"
				else
					resultado = "White"
				End if
		End Select
		return resultado
	End function
 
Function PintarPromedioTendencia(ByVal anio_3 As Decimal, ByVal anio_2 As Decimal, ByVal anio_1 As Decimal, ByVal PromedioTendencia As Decimal) As String
	Dim resultado As String = "WHITE"
					if(PromedioTendencia &gt; anio_3 AND PromedioTendencia &gt; anio_2 AND PromedioTendencia &gt; anio_1) then
