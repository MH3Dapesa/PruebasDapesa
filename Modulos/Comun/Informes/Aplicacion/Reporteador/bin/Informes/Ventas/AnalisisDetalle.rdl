  <DataSets>
    <DataSet Name="dsVentaDetalle">
      <Query>
        <DataSourceName>dsSIIL</DataSourceName>
        <QueryParameters>
          <QueryParameter Name=":pClaveCliente">
            <Value>=Parameters!pClaveCliente.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT R.MCA_CLAVE,M.DESCRIPCION AS MARCA,R.ANIO,SUM(R.IMPORTE) AS IMPORTE FROM (
	SELECT D.MCA_CLAVE,F.ANIO,SUM(D.IMPORTE) AS IMPORTE FROM
		(
			SELECT F.FOLFAC_FOLIO,F.NUMERO,EXTRACT(YEAR FROM F.FECHA) AS ANIO FROM FACTURAS_EMITIDAS F 
			WHERE F.CLI_CLAVE=''||:pClaveCliente||'' AND EXTRACT(YEAR FROM F.FECHA) BETWEEN EXTRACT(YEAR FROM SYSDATE)-3 AND EXTRACT(YEAR FROM SYSDATE) AND F.STATUS_CAN='N'
		) F,
		(SELECT D.FACT_EM_FOLFAC_FOLIO,D.FACT_EM_NUMERO,A.MCA_CLAVE,D.CANTIDAD*D.PRECIO AS IMPORTE FROM DETALLES_FACTURAS_EMITIDAS D, ARTICULOS A WHERE A.CLAVE=D.ART_CLAVE AND D.ART_CLAVE&lt;&gt;'SEGURO') D
	WHERE D.FACT_EM_FOLFAC_FOLIO=F.FOLFAC_FOLIO AND D.FACT_EM_NUMERO=F.NUMERO GROUP BY D.MCA_CLAVE,F.ANIO
	UNION
	SELECT D.MCA_CLAVE,N.ANIO,SUM(D.IMPORTE*-1) AS IMPORTE FROM
		(
			SELECT N.FOL_NCR_CT_FOLIO,N.NUMERO,EXTRACT(YEAR FROM N.FECHA) AS ANIO FROM NOTAS_CREDITO_AL_CTE N 
			WHERE N.CLI_CLAVE=''||:pClaveCliente||'' AND EXTRACT(YEAR FROM N.FECHA) BETWEEN EXTRACT(YEAR FROM SYSDATE)-3 AND EXTRACT(YEAR FROM SYSDATE) AND N.CONC_NOT_CLAVE IN (4,5,19,20,21,34) AND N.STATUS_CAN='N'
		) N,
		(SELECT D.NCR_AL_CTE_FOL_NCR_CT_FOLIO,D.NCR_AL_CTE_NUMERO,A.MCA_CLAVE,D.CANTIDAD*D.PRECIO AS IMPORTE FROM DETALLES_NOTAS_CREDITO_AL_CTE D, ARTICULOS A WHERE A.CLAVE=D.ART_CLAVE AND D.ART_CLAVE IS NOT NULL) D
	WHERE D.NCR_AL_CTE_FOL_NCR_CT_FOLIO=N.FOL_NCR_CT_FOLIO AND D.NCR_AL_CTE_NUMERO=N.NUMERO GROUP BY D.MCA_CLAVE,N.ANIO
) R, MARCAS M WHERE M.CLAVE=R.MCA_CLAVE GROUP BY R.MCA_CLAVE,M.DESCRIPCION,R.ANIO ORDER BY MARCA,ANIO</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
