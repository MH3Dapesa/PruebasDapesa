    <DataSet Name="dsVenta">
      <Query>
        <DataSourceName>dsSIIL</DataSourceName>
        <CommandText>SELECT C.CLAVE AS CLIENTE,C.RAZON_SOCIAL,C.VEND_CLAVE AS VENDEDOR,C.STATUS,ES.DESCRIPCION AS ESTADO,CI.DESCRIPCION AS CIUDAD,R.ANIO,SUM(R.IMPORTE) AS IMPORTE, C.GES_CLAVE FROM (
	SELECT F.CLI_CLAVE,F.ANIO,SUM(D.IMPORTE) AS IMPORTE FROM
		(SELECT S.CLI_CLAVE FROM SUC_CLIENTES S WHERE S.SUC_CLAVE=COALESCE(1,S.SUC_CLAVE)) S,
		(
			SELECT F.FOLFAC_FOLIO,F.NUMERO,F.CLI_CLAVE,EXTRACT(YEAR FROM F.FECHA) AS ANIO FROM FACTURAS_EMITIDAS F 
			WHERE F.CLI_CLAVE IN (
				SELECT A2.CLI_CLAVE FROM (
					SELECT R.CLI_CLAVE,SUM(R.IMPORTE) AS IMPORTE FROM (
						SELECT F.CLI_CLAVE,SUM(D.IMPORTE) AS IMPORTE FROM
							(SELECT S.CLI_CLAVE FROM SUC_CLIENTES S WHERE S.SUC_CLAVE=COALESCE(1,S.SUC_CLAVE)) S,
							(SELECT F.FOLFAC_FOLIO,F.NUMERO,F.CLI_CLAVE FROM FACTURAS_EMITIDAS F WHERE EXTRACT(YEAR FROM F.FECHA)=EXTRACT(YEAR FROM SYSDATE)-2 AND F.STATUS_CAN='N') F,
							(SELECT D.FACT_EM_FOLFAC_FOLIO,D.FACT_EM_NUMERO,D.CANTIDAD*D.PRECIO AS IMPORTE FROM DETALLES_FACTURAS_EMITIDAS D WHERE D.ART_CLAVE&lt;&gt;'SEGURO') D
						WHERE F.CLI_CLAVE=S.CLI_CLAVE AND D.FACT_EM_FOLFAC_FOLIO=F.FOLFAC_FOLIO AND D.FACT_EM_NUMERO=F.NUMERO GROUP BY F.CLI_CLAVE
						UNION
						SELECT N.CLI_CLAVE,SUM(D.IMPORTE*-1) AS IMPORTE FROM
							(SELECT S.CLI_CLAVE FROM SUC_CLIENTES S WHERE S.SUC_CLAVE=COALESCE(1,S.SUC_CLAVE)) S,
							(SELECT N.FOL_NCR_CT_FOLIO,N.NUMERO,N.CLI_CLAVE FROM NOTAS_CREDITO_AL_CTE N WHERE EXTRACT(YEAR FROM N.FECHA)=EXTRACT(YEAR FROM SYSDATE)-2 AND N.CONC_NOT_CLAVE IN (4,5,19,20,21,34) AND N.STATUS_CAN='N') N,
							(SELECT D.NCR_AL_CTE_FOL_NCR_CT_FOLIO,D.NCR_AL_CTE_NUMERO,D.CANTIDAD*D.PRECIO AS IMPORTE FROM DETALLES_NOTAS_CREDITO_AL_CTE D WHERE D.ART_CLAVE IS NOT NULL) D
						WHERE N.CLI_CLAVE=S.CLI_CLAVE AND D.NCR_AL_CTE_FOL_NCR_CT_FOLIO=N.FOL_NCR_CT_FOLIO AND D.NCR_AL_CTE_NUMERO=N.NUMERO GROUP BY  N.CLI_CLAVE
					) R GROUP BY R.CLI_CLAVE
				) A2 LEFT JOIN (
					SELECT R.CLI_CLAVE,SUM(R.IMPORTE) AS IMPORTE FROM (
						SELECT F.CLI_CLAVE,SUM(D.IMPORTE) AS IMPORTE FROM
							(SELECT S.CLI_CLAVE FROM SUC_CLIENTES S WHERE S.SUC_CLAVE=COALESCE(1,S.SUC_CLAVE)) S,
							(SELECT F.FOLFAC_FOLIO,F.NUMERO,F.CLI_CLAVE FROM FACTURAS_EMITIDAS F WHERE EXTRACT(YEAR FROM F.FECHA)=EXTRACT(YEAR FROM SYSDATE)-1 AND F.STATUS_CAN='N') F,
							(SELECT D.FACT_EM_FOLFAC_FOLIO,D.FACT_EM_NUMERO,D.CANTIDAD*D.PRECIO AS IMPORTE FROM DETALLES_FACTURAS_EMITIDAS D WHERE D.ART_CLAVE&lt;&gt;'SEGURO') D
						WHERE F.CLI_CLAVE=S.CLI_CLAVE AND D.FACT_EM_FOLFAC_FOLIO=F.FOLFAC_FOLIO AND D.FACT_EM_NUMERO=F.NUMERO GROUP BY F.CLI_CLAVE
						UNION
						SELECT N.CLI_CLAVE,SUM(D.IMPORTE*-1) AS IMPORTE FROM
							(SELECT S.CLI_CLAVE FROM SUC_CLIENTES S WHERE S.SUC_CLAVE=COALESCE(1,S.SUC_CLAVE)) S,
							(SELECT N.FOL_NCR_CT_FOLIO,N.NUMERO,N.CLI_CLAVE FROM NOTAS_CREDITO_AL_CTE N WHERE EXTRACT(YEAR FROM N.FECHA)=EXTRACT(YEAR FROM SYSDATE)-1 AND N.CONC_NOT_CLAVE IN (4,5,19,20,21,34) AND N.STATUS_CAN='N') N,
							(SELECT D.NCR_AL_CTE_FOL_NCR_CT_FOLIO,D.NCR_AL_CTE_NUMERO,D.CANTIDAD*D.PRECIO AS IMPORTE FROM DETALLES_NOTAS_CREDITO_AL_CTE D WHERE D.ART_CLAVE IS NOT NULL) D
						WHERE N.CLI_CLAVE=S.CLI_CLAVE AND D.NCR_AL_CTE_FOL_NCR_CT_FOLIO=N.FOL_NCR_CT_FOLIO AND D.NCR_AL_CTE_NUMERO=N.NUMERO GROUP BY  N.CLI_CLAVE
					) R GROUP BY R.CLI_CLAVE
				) A1 ON A2.CLI_CLAVE=A1.CLI_CLAVE 
			) AND EXTRACT(YEAR FROM F.FECHA) BETWEEN EXTRACT(YEAR FROM SYSDATE)-3 AND EXTRACT(YEAR FROM SYSDATE) AND F.STATUS_CAN='N'
		) F,
		(SELECT D.FACT_EM_FOLFAC_FOLIO,D.FACT_EM_NUMERO,D.CANTIDAD*D.PRECIO AS IMPORTE FROM DETALLES_FACTURAS_EMITIDAS D WHERE D.ART_CLAVE&lt;&gt;'SEGURO') D
	WHERE F.CLI_CLAVE=S.CLI_CLAVE AND D.FACT_EM_FOLFAC_FOLIO=F.FOLFAC_FOLIO AND D.FACT_EM_NUMERO=F.NUMERO GROUP BY F.CLI_CLAVE,F.ANIO
	UNION
	SELECT N.CLI_CLAVE,N.ANIO,SUM(D.IMPORTE*-1) AS IMPORTE FROM
		(SELECT S.CLI_CLAVE FROM SUC_CLIENTES S WHERE S.SUC_CLAVE=COALESCE(1,S.SUC_CLAVE)) S,
		(
			SELECT N.FOL_NCR_CT_FOLIO,N.NUMERO,N.CLI_CLAVE,EXTRACT(YEAR FROM N.FECHA) AS ANIO FROM NOTAS_CREDITO_AL_CTE N 
			WHERE N.CLI_CLAVE IN (
				SELECT A2.CLI_CLAVE FROM (
					SELECT R.CLI_CLAVE,SUM(R.IMPORTE) AS IMPORTE FROM (
						SELECT F.CLI_CLAVE,SUM(D.IMPORTE) AS IMPORTE FROM
							(SELECT S.CLI_CLAVE FROM SUC_CLIENTES S WHERE S.SUC_CLAVE=COALESCE(1,S.SUC_CLAVE)) S,
							(SELECT F.FOLFAC_FOLIO,F.NUMERO,F.CLI_CLAVE FROM FACTURAS_EMITIDAS F WHERE EXTRACT(YEAR FROM F.FECHA)=EXTRACT(YEAR FROM SYSDATE)-2 AND F.STATUS_CAN='N') F,
							(SELECT D.FACT_EM_FOLFAC_FOLIO,D.FACT_EM_NUMERO,D.CANTIDAD*D.PRECIO AS IMPORTE FROM DETALLES_FACTURAS_EMITIDAS D WHERE D.ART_CLAVE&lt;&gt;'SEGURO') D
						WHERE F.CLI_CLAVE=S.CLI_CLAVE AND D.FACT_EM_FOLFAC_FOLIO=F.FOLFAC_FOLIO AND D.FACT_EM_NUMERO=F.NUMERO GROUP BY F.CLI_CLAVE
						UNION
						SELECT N.CLI_CLAVE,SUM(D.IMPORTE*-1) AS IMPORTE FROM
							(SELECT S.CLI_CLAVE FROM SUC_CLIENTES S WHERE S.SUC_CLAVE=COALESCE(1,S.SUC_CLAVE)) S,
							(SELECT N.FOL_NCR_CT_FOLIO,N.NUMERO,N.CLI_CLAVE FROM NOTAS_CREDITO_AL_CTE N WHERE EXTRACT(YEAR FROM N.FECHA)=EXTRACT(YEAR FROM SYSDATE)-2 AND N.CONC_NOT_CLAVE IN (4,5,19,20,21,34) AND N.STATUS_CAN='N') N,
							(SELECT D.NCR_AL_CTE_FOL_NCR_CT_FOLIO,D.NCR_AL_CTE_NUMERO,D.CANTIDAD*D.PRECIO AS IMPORTE FROM DETALLES_NOTAS_CREDITO_AL_CTE D WHERE D.ART_CLAVE IS NOT NULL) D
						WHERE N.CLI_CLAVE=S.CLI_CLAVE AND D.NCR_AL_CTE_FOL_NCR_CT_FOLIO=N.FOL_NCR_CT_FOLIO AND D.NCR_AL_CTE_NUMERO=N.NUMERO GROUP BY  N.CLI_CLAVE
					) R GROUP BY R.CLI_CLAVE
				) A2 LEFT JOIN (
					SELECT R.CLI_CLAVE,SUM(R.IMPORTE) AS IMPORTE FROM (
						SELECT F.CLI_CLAVE,SUM(D.IMPORTE) AS IMPORTE FROM
							(SELECT S.CLI_CLAVE FROM SUC_CLIENTES S WHERE S.SUC_CLAVE=COALESCE(1,S.SUC_CLAVE)) S,
							(SELECT F.FOLFAC_FOLIO,F.NUMERO,F.CLI_CLAVE FROM FACTURAS_EMITIDAS F WHERE EXTRACT(YEAR FROM F.FECHA)=EXTRACT(YEAR FROM SYSDATE)-1 AND F.STATUS_CAN='N') F,
							(SELECT D.FACT_EM_FOLFAC_FOLIO,D.FACT_EM_NUMERO,D.CANTIDAD*D.PRECIO AS IMPORTE FROM DETALLES_FACTURAS_EMITIDAS D WHERE D.ART_CLAVE&lt;&gt;'SEGURO') D
						WHERE F.CLI_CLAVE=S.CLI_CLAVE AND D.FACT_EM_FOLFAC_FOLIO=F.FOLFAC_FOLIO AND D.FACT_EM_NUMERO=F.NUMERO GROUP BY F.CLI_CLAVE
						UNION
						SELECT N.CLI_CLAVE,SUM(D.IMPORTE*-1) AS IMPORTE FROM
							(SELECT S.CLI_CLAVE FROM SUC_CLIENTES S WHERE S.SUC_CLAVE=COALESCE(1,S.SUC_CLAVE)) S,
							(SELECT N.FOL_NCR_CT_FOLIO,N.NUMERO,N.CLI_CLAVE FROM NOTAS_CREDITO_AL_CTE N WHERE EXTRACT(YEAR FROM N.FECHA)=EXTRACT(YEAR FROM SYSDATE)-1 AND N.CONC_NOT_CLAVE IN (4,5,19,20,21,34) AND N.STATUS_CAN='N') N,
							(SELECT D.NCR_AL_CTE_FOL_NCR_CT_FOLIO,D.NCR_AL_CTE_NUMERO,D.CANTIDAD*D.PRECIO AS IMPORTE FROM DETALLES_NOTAS_CREDITO_AL_CTE D WHERE D.ART_CLAVE IS NOT NULL) D
						WHERE N.CLI_CLAVE=S.CLI_CLAVE AND D.NCR_AL_CTE_FOL_NCR_CT_FOLIO=N.FOL_NCR_CT_FOLIO AND D.NCR_AL_CTE_NUMERO=N.NUMERO GROUP BY  N.CLI_CLAVE
					) R GROUP BY R.CLI_CLAVE
				) A1 ON A2.CLI_CLAVE=A1.CLI_CLAVE 
			) AND EXTRACT(YEAR FROM N.FECHA) BETWEEN EXTRACT(YEAR FROM SYSDATE)-3 AND EXTRACT(YEAR FROM SYSDATE) AND N.CONC_NOT_CLAVE IN (4,5,19,20,21,34) AND N.STATUS_CAN='N'
			) N,
			(SELECT D.NCR_AL_CTE_FOL_NCR_CT_FOLIO,D.NCR_AL_CTE_NUMERO,D.CANTIDAD*D.PRECIO AS IMPORTE FROM DETALLES_NOTAS_CREDITO_AL_CTE D WHERE D.ART_CLAVE IS NOT NULL) D
			WHERE N.CLI_CLAVE=S.CLI_CLAVE AND D.NCR_AL_CTE_FOL_NCR_CT_FOLIO=N.FOL_NCR_CT_FOLIO AND D.NCR_AL_CTE_NUMERO=N.NUMERO GROUP BY  N.CLI_CLAVE,N.ANIO
			) R, CLIENTES C,ESTADOS ES, CIUDADES CI WHERE C.CLAVE=R.CLI_CLAVE AND C.CD_CLAVE = CI.CLAVE AND C.EDO_CLAVE=ES.CLAVE GROUP BY C.CLAVE,C.RAZON_SOCIAL,C.VEND_CLAVE,C.STATUS,ES.DESCRIPCION,CI.DESCRIPCION,R.ANIO,C.GES_CLAVE ORDER BY LPAD(CLIENTE,20),ANIO</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
