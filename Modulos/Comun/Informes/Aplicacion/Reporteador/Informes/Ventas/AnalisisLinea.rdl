<Report xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner" xmlns:cl="http://schemas.microsoft.com/sqlserver/reporting/2010/01/componentdefinition" xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/reportdefinition">
  <AutoRefresh>0</AutoRefresh>
  <DataSources>
    <DataSource Name="dsSIIL">
      <ConnectionProperties>
        <DataProvider>ORACLE</DataProvider>
        <ConnectString>Data Source=SIIL;Unicode=True</ConnectString>
        <Prompt>Especifique un nombre de usuario y una contraseña para el origen de datos dsSIIL:</Prompt>
      </ConnectionProperties>
      <rd:DataSourceID>0dd1ab81-5eb5-4b5e-b01c-7e714a7c1ac1</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="dsVentaDetalle">
      <Query>
        <DataSourceName>dsSIIL</DataSourceName>
        <QueryParameters>
          <QueryParameter Name=":pClaveCliente">
            <Value>=Parameters!pClaveCliente.Value</Value>
          </QueryParameter>
          <QueryParameter Name=":pClaveLinea">
            <Value>=Parameters!pClaveLinea.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
			SELECT R.ART_CLAVE,Ar.DESCRIPCION,R.LIN_ART_CLAVE, L.DESCRIPCION AS LINEA,R.ANIO,SUM(R.IMPORTE) AS IMPORTE FROM (
			SELECT D.ART_CLAVE,D.LIN_ART_CLAVE,F.ANIO,SUM(D.IMPORTE) AS IMPORTE FROM
			(
			SELECT F.FOLFAC_FOLIO,F.NUMERO,F.CLI_CLAVE,EXTRACT(YEAR FROM F.FECHA) AS ANIO FROM FACTURAS_EMITIDAS F
			WHERE F.CLI_CLAVE=''||:pClaveCliente||'' AND EXTRACT(YEAR FROM F.FECHA) BETWEEN EXTRACT(YEAR FROM SYSDATE)-3 AND EXTRACT(YEAR FROM SYSDATE) AND F.STATUS_CAN='N'
			) F,
			(
			SELECT D.FACT_EM_FOLFAC_FOLIO,D.FACT_EM_NUMERO,A.LIN_ART_CLAVE,A.CLAVE AS ART_CLAVE,D.CANTIDAD*D.PRECIO AS IMPORTE
			FROM DETALLES_FACTURAS_EMITIDAS D, ARTICULOS A
			WHERE A.CLAVE=D.ART_CLAVE AND A.LIN_ART_CLAVE=COALESCE(''||:pClaveLinea||'',A.LIN_ART_CLAVE)
			AND D.ART_CLAVE!='SEGURO'
			) D
			WHERE D.FACT_EM_FOLFAC_FOLIO=F.FOLFAC_FOLIO AND D.FACT_EM_NUMERO=F.NUMERO GROUP BY D.LIN_ART_CLAVE,D.ART_CLAVE,F.ANIO
			UNION
			SELECT D.CLAVE,D.LIN_ART_CLAVE,N.ANIO,SUM(D.IMPORTE*-1) AS IMPORTE FROM
			(
			SELECT N.FOL_NCR_CT_FOLIO,N.NUMERO,N.CLI_CLAVE,EXTRACT(YEAR FROM N.FECHA) AS ANIO FROM NOTAS_CREDITO_AL_CTE N
			WHERE N.CLI_CLAVE=''||:pClaveCliente||'' AND EXTRACT(YEAR FROM N.FECHA) BETWEEN EXTRACT(YEAR FROM SYSDATE)-3 AND EXTRACT(YEAR FROM SYSDATE) AND N.CONC_NOT_CLAVE IN (4,5,19,20,21,34) AND N.STATUS_CAN='N'
			) N,
			(
			SELECT D.NCR_AL_CTE_FOL_NCR_CT_FOLIO,D.NCR_AL_CTE_NUMERO,A.LIN_ART_CLAVE,A.CLAVE,D.CANTIDAD*D.PRECIO AS IMPORTE
			FROM DETALLES_NOTAS_CREDITO_AL_CTE D, ARTICULOS A
			WHERE A.CLAVE=D.ART_CLAVE AND A.LIN_ART_CLAVE=COALESCE(''||:pClaveLinea||'',A.LIN_ART_CLAVE) AND D.ART_CLAVE IS NOT NULL
			) D
			WHERE D.NCR_AL_CTE_FOL_NCR_CT_FOLIO=N.FOL_NCR_CT_FOLIO AND D.NCR_AL_CTE_NUMERO=N.NUMERO GROUP BY D.LIN_ART_CLAVE,D.CLAVE,N.ANIO
			) R, LINEAS_ARTICULOS L,Articulos Ar WHERE  L.CLAVE = R.LIN_ART_CLAVE AND AR.CLAVE=R.ART_CLAVE
			GROUP BY R.ART_CLAVE,Ar.DESCRIPCION,R.LIN_ART_CLAVE,L.DESCRIPCION,R.ANIO ORDER BY ANIO,Ar.DESCRIPCION
		</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
